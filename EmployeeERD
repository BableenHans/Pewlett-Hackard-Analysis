Departments
-
dept_no varchar pk fk - dept_emp.dept_no
dept_name varchar

Employees
-
emp_no int 
birth_date date
first_name varchar 
last_name varchar
gender varchar
hire_date date

dept_emp
-
dept_no 
emp_no int fk -< Employees.emp_no
from_date
to_date


dept_managers
-
dept_no varchar pk fk - Departments.dept_no
emp_no int pk 
from_date
to_date

salaries
-
emp_no int pk fk - Employees.emp_no
salary
from_date
to_date

titles
-
emp_no int pk fk -< Employees.emp_no 
title pk
from_date pk 
to_date
